            <Font size="1.0" />
         </font></Button>
      <TextField fx:id="txt_placa" layoutX="17.0" layoutY="56.0" prefHeight="25.0" prefWidth="103.0" promptText="PLACA" styleClass="mainFxmlClass" stylesheets="@../css/box_user_pass.css" />
