         <font>
            <Font size="1.0" />
         </font></Button>
      <TableView fx:id="tabela" layoutX="154.0" prefHeight="600.0" prefWidth="646.0">
        <columns>
          <TableColumn fx:id="ID" prefWidth="24.0" text="ID" />
          <TableColumn fx:id="PLACA" prefWidth="115.0" text="PLACA" />
            <TableColumn fx:id="DATA" prefWidth="100.0" text="DATA" />
            <TableColumn fx:id="ENTRADA" prefWidth="100.0" text="ENTRADA" />
            <TableColumn fx:id="SAIDA" prefWidth="100.0" text="SAÍDA" />
            <TableColumn fx:id="TOTAL" prefWidth="100.0" text="TOTAL" />
