<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="mainFxmlClass" stylesheets="@../css/tela_entrada.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EntradaController">
   <children>
      <Button layoutX="38.0" layoutY="6.0" mnemonicParsing="false" onAction="#back" opacity="0.0" prefHeight="19.0" prefWidth="73.0" text="VOLTAR">
